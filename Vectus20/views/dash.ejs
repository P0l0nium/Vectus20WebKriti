            visibility: hidden;
        }


        /*
        .reg ul {
            margin: 0;
        }
*/

        .reg ul {
            margin: 0;
            vertical-align: middle;
            line-height: 5vh;
        }

        .hack {
            /*            display: block;*/
            font-family: fantasy;
        }

        .game {
            /*            display: none;*/
            font-family: fantasy;
        }

        .play {
            /*            display: none;*/
            font-family: fantasy;
        }

        .logout {
            background-color: #4CAF50;
            border: none;
            color: white;
            padding: 16px 32px;
            text-align: center;
            text-decoration: none;
            display: inline-block;
            font-size: 16px;
            margin: 4px 2px;
            transition-duration: 0.4s;
            cursor: pointer;
            background-color: white;
            color: black;
            border: 2px solid #008CBA;
            border-radius: 45%;
        }

        .logout:hover {
            background-color: #008CBA;
            color: white;
        }

        li {
            list-style-type: none;
        }

        .unreg-btn {
            height: 5vh;
            font-size: 2.5vh;
            border: solid 2px #00b7ff;
            transition: all 1s;
            cursor: pointer;
        }

        .unreg-btn:hover {
            background-color: black;
            color: white;
        }

        .modal {
            display: none;
            position: fixed;
            z-index: 1;
            padding-top: 100px;
            left: 0;
            top: 0;
            width: 100%;
            height: 100%;
            overflow: auto;
            background-color: rgb(0, 0, 0);
            background-color: rgba(0, 0, 0, 0.4);
        }


        .modal-content {
            background: rgb(103, 13, 68);
            background: linear-gradient(113deg, rgba(103, 13, 68, 1) 0%, rgba(70, 4, 64, 1) 54%, rgba(41, 1, 33, 1) 100%);
            margin: auto;
            padding: 20px;
            border: 1px solid #888;
            width: 30%;
        }

        .close {
            color: #aaaaaa;
            float: right;
            font-size: 28px;
            font-weight: bold;
        }

        .close:hover,
        .close:focus {
            color: #000;
            text-decoration: none;
            cursor: pointer;
        }

        .Yes {
